							"listen": "test",
							"script": {
								"exec": [
									"pm.test(\"Status code must be 200\",()=>{\r",
									"    pm.expect(pm.response.code).to.eql(200);\r",
									"});\r",
									"\r",
									"pm.test(\"All products must be present\",()=>{\r",
									"    const body = pm.response.json().products;\r",
									"    let i = 0;\r",
									"    body.forEach(product=>{\r",
									"        if(product.id){\r",
									"            i++;\r",
									"        }\r",
									"    })\r",
									"    console.log(i);\r",
									"    pm.expect(i).to.eql(30);\r",
									"});\r",
									"\r",
									"pm.environment.set(\"productId\",pm.response.json().products[19].id);\r",
									"\r",
									"pm.environment.set(\"product18\",JSON.stringify(pm.response.json().products[19]));"
